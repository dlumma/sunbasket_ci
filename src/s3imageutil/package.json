  ],
  "author": "Denali Lumma <denali@bloomcode.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bloomcode/s3imageutil/issues"
  },
  "homepage": "https://github.com/bloomcode/s3imageutil/blob/master/README.md",
  "dependencies": {
    "aws-sdk": "^2.166.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  },
  "ava": {
